@model IEnumerable<SunshineInstitute.Models.FeePayment>

@{
    ViewBag.Title = "AllDetail";
}

<h2>All Fee Detail</h2>

<p>
    @Html.ActionLink("Fee Pay", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Contact)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fee)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateOfPayment)
        </th>
        <th>Update </th>
        <th>Delete </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Contact)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fee)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateOfPayment)
            </td>
            <td>
                <span class="btn btn-danger">@Html.ActionLink("Edit", "Edit", new { id = item.id }) </span>
            </td>
            <td>
                <span class="btn btn-danger"> @Html.ActionLink("Delete", "Delete", new { id = item.id }) </span>
            </td>
        </tr>
    }

</table>



<hr />
<a href="@Url.Action("AdminWork","Login")" class="btn btn-success">BAck to Dashboard </a>
